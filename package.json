{
  "name": "blog",
  "description": "25sprout web starter with React",
  "version": "0.1.0",
  "keywords": [
    "react",
    "25sprout",
    "starter"
  ],
  "contributor": [
    {
      "name": "25sprout Front-End Team",
      "email": "hey@25sprout.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@nas25lol.myqnapcloud.com:12222/frontend/react-starter.git"
  },
  "bugs": {
    "url": "http://nas25lol.myqnapcloud.com:10088/frontend/react-starter/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "babel-node server.js",
    "dev": "babel-node server.js",
    "demo": "cross-env NODE_ENV=demo webpack --config webpack.config.prod.babel.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.babel.js",
    "lint": "eslint . && stylelint .",
    "storybook": "start-storybook -p 8000 -c storybook",
    "test": "jest --coverage"
  },
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "dependencies": {
    "react": "^15.4.1",
    "react-container-helper": "^0.2.2",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.25.0",
    "jest": "^18.1.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.1",
    "redux-logger": "^2.7.4",
    "storybook-addon-specifications": "^1.0.15",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "svg-css-modules-loader": "^1.3.0",
    "svg-react-loader": "^0.3.7",
    "svgo-loader": "^1.1.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/storybook/__conf__/mockConfig.js"
    ],
    "testRegex": "../.*.stories.js$",
    "automock": false,
    "globals": {
      "__TESTS__": true
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/storybook/__mocks__/file.js",
      "\\.(css|less)$": "<rootDir>/storybook/__mocks__/style.js"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/storybook/",
      "<rootDir>/node_modules/"
    ]
  }
}
